import java.util.Scanner;

public class MaximumOfThreeNumbers {
    public static void main(String[] args) {
        int a, b, c;
        Scanner scanner = new Scanner(System.in);
        a = scanner.nextInt();
        b = scanner.nextInt();
        c = scanner.nextInt();
        if( a > b && a > c)
            System.out.print(a);
        else if (b > c)
            System.out.print(b);
        else
            System.out.print(c);
    }
}
