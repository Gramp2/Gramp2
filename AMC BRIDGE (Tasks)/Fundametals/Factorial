import java.util.Scanner;

public class Factorial {
    public static void main(String[] args) {
        int n;
        Scanner scanner = new Scanner(System.in);
        n = scanner.nextInt();
        fact(n);
    }
    static void fact(int n){
        int result = 1;
        for(int i = 1; i <= n; i++)
            result=result*i;
        System.out.print(result);
    }
}
