import java.util.Scanner;

public class Cube {
    public static void main(String[] args) {
        int a;
        int V = 0;
        int S = 0;
        Scanner scanner = new Scanner(System.in);
        a = scanner.nextInt();
        CUBE(a,V,S);
    }
    static void CUBE(int a, int V, int S){
        V=a*a*a;
        S = 6*a*a;
        System.out.print(V + " " + S);
    }
}
